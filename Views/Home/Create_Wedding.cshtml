@model Wedding

<div class="text-right">
    <p><a class="btn mt-2 float-end" href="/Dashboard">Dashboard</a></p>
    <p><a class="btn mt-2 float-end" href="/">Log Out</a></p>
</div>
<div class="text-center">
    <div class="row">
        <div class="col-12">
            <h1>Plan Your Wedding</h1>
            <form action="/create/wedding" method="post">
                <div class="form-group">
                    <label for="Wedder_One">Wedder One</label>
                    <input asp-for="Wedder_One" class="form-control">
                    <span asp-validation-for="Wedder_One" class="text=danger"></span>
                </div>
                <div class="form-group">
                    <label for="Wedder_Two">Wedder Two</label>
                    <input asp-for="Wedder_Two" class="form-control">
                    <span asp-validation-for="Wedder_Two" class="text=danger"></span>
                </div>
                <div class="form-group">
                    <label for="Date">Wedding Date</label>
                    <input asp-for="Date" class="form-control">
                    <span asp-validation-for="Date" class="text=danger"></span>
                </div>
                <div class="form-group">
                    <label for="Wedding_Address">Wedding Address</label>
                    <input asp-for="Wedding_Address" class="form-control">
                    <span asp-validation-for="Wedding_Address" class="text=danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-prmary">
                </div>
            </form>
        </div>
    </div>
</div>